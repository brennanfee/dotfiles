[Appearance]
BoldIntense=true
ColorScheme=onehalf-dark
Font=JetBrainsMono Nerd Font,16,-1,5,50,0,0,0,0,0
UseFontLineChararacters=true

[General]
Command=tmux
Directory=~/profile
Name=MyTerminal
Parent=FALLBACK/
TerminalCenter=true
TerminalColumns=122
TerminalMargin=5
TerminalRows=32

[Interaction Options]
MiddleClickPasteMode=1
OpenLinksByDirectClickEnabled=true
TrimTrailingSpacesInSelectedText=true
UnderlineFilesEnabled=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=2
